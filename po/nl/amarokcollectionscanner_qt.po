#: utilities/collectionscanner/CollectionScanner.cpp:1008
msgid "Usage: amarokcollectionscanner [options] <Folder(s)>"
msgstr "Gebruik: amarokcollectionscanner [opties] <map(pen)>"

#: utilities/collectionscanner/CollectionScanner.cpp:1009
msgid "User-modifiable Options:"
msgstr "Opties door gebruiker te wijzigen:"

#: utilities/collectionscanner/CollectionScanner.cpp:1010
msgid ""
"Folder(s)             : Space-separated list of folders to scan; when using -"
"b and -i, the path to the file generated by Amarok containing the list of "
"folders"
msgstr ""
"Map(pen)             : spatiegescheiden lijst van te doorlopen mappen; bij "
"bebruik van -b en -i, het pad naar het bestand dat door Amarok is "
"gegenereerd die de lijst met mappen bevat"

#: utilities/collectionscanner/CollectionScanner.cpp:1011
msgid "-h, --help            : This help text"
msgstr "-h, --help            : Deze helptekst"

#: utilities/collectionscanner/CollectionScanner.cpp:1012
msgid "-r, --recursive       : Scan folders recursively"
msgstr "-r, --recursive       : Mappen recursief doorzoeken"

#: utilities/collectionscanner/CollectionScanner.cpp:1013
msgid "-i, --incremental     : Incremental scan (modified folders only)"
msgstr "-i, --incremental     : Incrementele scan (alleen gewijzigde mappen)"

#: utilities/collectionscanner/CollectionScanner.cpp:1014
msgid "-p, --importplaylists : Import playlists"
msgstr "-p, --importplaylists : Afspeellijsten importeren"

#: utilities/collectionscanner/CollectionScanner.cpp:1015
msgid ""
"-s, --restart         : After a crash, restart the scanner in its last "
"position"
msgstr ""
"-s, --restart         : De scanner na een crash naar zijn laatste positie "
"herstellen"

#: utilities/collectionscanner/CollectionScanner.cpp:1016
msgid "-b, --batch           : Run in batch mode"
msgstr "-b, --batch           : Uitvoeren in batchmodus"

#: utilities/collectionscanner/CollectionScanner.cpp:1017
msgid "-c, --nocharset       : Use the charset detector on ID3 tags"
msgstr ""

#: utilities/collectionscanner/CollectionScanner.cpp:1018
msgid ""
"--rpath=\"<path>\"      : In full-scan batch mode, specifies a path to "
"prepend to entries (default is the current directory)"
msgstr ""
"--rpath=\"<path>\"      : specifieert in full-scan batchmodus een pad om aan "
"ingangen vooraf te laten gaan (standaard is de huidige map)"
