#: utilities/collectionscanner/CollectionScanner.cpp:1002
msgid "Amarok Collection Scanner"
msgstr "Amarok Koleksiyon Tarayıcı"

#: utilities/collectionscanner/CollectionScanner.cpp:1003
msgid ""
"Note: For debugging purposes this application can be invoked from the "
"command line,\n"
"but it will not actually build a collection this way without the Amarok "
"player."
msgstr ""

#: utilities/collectionscanner/CollectionScanner.cpp:1004
msgid ""
"IRC:\n"
"server: irc.freenode.net / channels: #amarok, #amarok.de, #amarok.es, "
"#amarok.fr\n"
"\n"
"Feedback:\n"
"amarok@kde.org"
msgstr ""
"IRC:\n"
"server: irc.freenode.net / channels: #amarok, #amarok.de, #amarok.es, "
"#amarok.fr\n"
"\n"
"Geribildirim:\n"
"amarok@kde.org"

#: utilities/collectionscanner/CollectionScanner.cpp:1005
msgid "Usage: amarokcollectionscanner [options] <Folder(s)>"
msgstr "Kullanım: amarokcollectionscanner [seçenekler] <Dizin(ler)>"

#: utilities/collectionscanner/CollectionScanner.cpp:1006
msgid "User-modifiable Options:"
msgstr "Kullanıcı Tarafından Düzenlenebilir Seçenekler:"

#: utilities/collectionscanner/CollectionScanner.cpp:1007
msgid ""
"Folder(s)             : Space-separated list of folders to scan; when using -"
"b and -i, the path to the file generated by Amarok containing the list of "
"folders"
msgstr ""

#: utilities/collectionscanner/CollectionScanner.cpp:1008
msgid "-h, --help            : This help text"
msgstr "-h, --help            : Bu yardım metni"

#: utilities/collectionscanner/CollectionScanner.cpp:1009
msgid "-r, --recursive       : Scan folders recursively"
msgstr "-r, --recursive       : Dizinleri özyinelemeli olarak tara"

#: utilities/collectionscanner/CollectionScanner.cpp:1010
msgid "-i, --incremental     : Incremental scan (modified folders only)"
msgstr ""
"-i, --incremental     : Artımsal tarama (sadece düzenlenen dizinler için)"

#: utilities/collectionscanner/CollectionScanner.cpp:1011
msgid "-p, --importplaylists : Import playlists"
msgstr "-p, --importplaylists : Parça listelerini içeriye aktar"

#: utilities/collectionscanner/CollectionScanner.cpp:1012
msgid ""
"-s, --restart         : After a crash, restart the scanner in its last "
"position"
msgstr ""
"-s, --restart         : Bir çökmeden sonra tarayıcıyı son kaldığı yerden "
"yeniden başlat"

#: utilities/collectionscanner/CollectionScanner.cpp:1013
msgid "-b, --batch           : Run in batch mode"
msgstr "-b, --batch           : Çoklu kipte çalıştır"

#: utilities/collectionscanner/CollectionScanner.cpp:1014
msgid ""
"--rpath=\"<path>\"      : In full-scan batch mode, specifies a path to "
"prepend to entries (default is the current directory)"
msgstr ""
