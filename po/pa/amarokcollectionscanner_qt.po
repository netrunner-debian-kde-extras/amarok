#: utilities/collectionscanner/CollectionScanner.cpp:1034
msgid "Amarok Collection Scanner"
msgstr "ਅਮਰੋਕ ਭੰਡਾਰ ਸਕੈਨਰ"

#: utilities/collectionscanner/CollectionScanner.cpp:1035
msgid ""
"Note: For debugging purposes this application can be invoked from the "
"command line,\n"
"but it will not actually build a collection this way without the Amarok "
"player."
msgstr ""

#: utilities/collectionscanner/CollectionScanner.cpp:1036
msgid ""
"IRC:\n"
"server: irc.freenode.net / channels: #amarok, #amarok.de, #amarok.es, "
"#amarok.fr\n"
"\n"
"Feedback:\n"
"amarok@kde.org"
msgstr ""
"IRC:\n"
"ਸਰਵਰ: irc.freenode.net / channels: #amarok, #amarok.de, #amarok.es, #amarok."
"fr\n"
"\n"
"ਸੁਝਾਅ:\n"
"amarok@kde.org"

#: utilities/collectionscanner/CollectionScanner.cpp:1037
msgid "Usage: amarokcollectionscanner [options] <Folder(s)>"
msgstr "ਵਰਤੋਂ: amarokcollectionscanner [options] <Folder(s)>"

#: utilities/collectionscanner/CollectionScanner.cpp:1038
msgid "User-modifiable Options:"
msgstr "ਯੂਜ਼ਰ ਵਲੋਂ ਸੋਧਣਯੋਗ ਚੋਣਾਂ:"

#: utilities/collectionscanner/CollectionScanner.cpp:1039
msgid ""
"Folder(s)             : Space-separated list of folders to scan; when using -"
"b and -i, the path to the file generated by Amarok containing the list of "
"folders"
msgstr ""

#: utilities/collectionscanner/CollectionScanner.cpp:1040
msgid "-h, --help            : This help text"
msgstr "-h, --help            : This help text"

#: utilities/collectionscanner/CollectionScanner.cpp:1041
msgid "-v, --version         : Print the version of this tool"
msgstr "-v, --version         : Print the version of this tool"

#: utilities/collectionscanner/CollectionScanner.cpp:1042
msgid "-r, --recursive       : Scan folders recursively"
msgstr "-r, --recursive       : Scan folders recursively"

#: utilities/collectionscanner/CollectionScanner.cpp:1043
msgid "-i, --incremental     : Incremental scan (modified folders only)"
msgstr "-i, --incremental     : Incremental scan (modified folders only)"

#: utilities/collectionscanner/CollectionScanner.cpp:1044
msgid "-p, --importplaylists : Import playlists"
msgstr "-p, --importplaylists : Import playlists"

#: utilities/collectionscanner/CollectionScanner.cpp:1045
msgid ""
"-s, --restart         : After a crash, restart the scanner in its last "
"position"
msgstr ""
"-s, --restart         : After a crash, restart the scanner in its last "
"position"

#: utilities/collectionscanner/CollectionScanner.cpp:1046
msgid "-b, --batch           : Run in batch mode"
msgstr "-b, --batch           : Run in batch mode"

#: utilities/collectionscanner/CollectionScanner.cpp:1047
msgid "--idlepriority        : Run at idle priority"
msgstr ""

#: utilities/collectionscanner/CollectionScanner.cpp:1048
msgid ""
"--rpath=\"<path>\"      : In full-scan batch mode, specifies a path to "
"prepend to entries (default is the current directory)"
msgstr ""
"--rpath=\"<path>\"      : In full-scan batch mode, specifies a path to "
"prepend to entries (default is the current directory)"

#: utilities/collectionscanner/CollectionScanner.cpp:1049
msgid "--savelocation        : Internal command used by Amarok"
msgstr "--savelocation        : Internal command used by Amarok"

#~ msgid "-c, --nocharset       : Use the charset detector on ID3 tags"
#~ msgstr "-c, --nocharset       : Use the charset detector on ID3 tags"
